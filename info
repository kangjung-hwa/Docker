# docker 실행
docker start -ai jh_pytorch
exec

# docker mount
docker run -itd --name jp_pytorch --ipc=host -v /home/jung//:/workspace/ -p 111:111 --gpus all --restart=always pytorch/pytorch

# docker container 확인
docker ps
docker ps -a

#docker 삭제
docker rm [컨테이너 이름]

#exec 명령어사용을 통한 백그라운드 실행 컨테이너 내부 접속
docker exec -i -t wordpressdb /bin/bash
